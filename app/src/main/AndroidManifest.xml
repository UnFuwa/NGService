<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.unfuwa.ngservice">

    <uses-permission android:name="android.permission.CALL_PHONE"/>

    <application
        android:icon="@drawable/icon_launch"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.activity.client.MainClientActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activity.general.RegistrationActivity" />
        <activity android:name=".ui.activity.general.AuthorizationActivity"/>
        <activity android:name=".ui.activity.general.LoadScreen" />
    </application>

</manifest>